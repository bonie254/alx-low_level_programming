#!/bin/bash

# Loop through all files in the current directory
for file in *; do
    # Check if the file is a regular file
    if [ -f "$file" ]; then
        # Use sed to replace 'main.h' with 'main.h' in the file
        sed -i 's/holberton\.h/main\.h/g' "$file"
        echo "Replaced in file: $file"
    fi
done

